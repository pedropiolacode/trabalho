
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id Int @id @unique @default(autoincrement())
  username String @unique
  password String
  isAdmin Boolean @default(false)
}

model Book {
  id Int @id @default(autoincrement())
  title String
  autor String
  available Boolean @default(true)
}

